{
  "name": "microgen-v3-sdk",
  "version": "0.16.1",
  "description": "Microgen SDK",
  "main": "dist/main/index.js",
  "module": "dist/module/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/main/index.js",
      "import": "./dist/module/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mejik-dev/microgen-v3-sdk.git",
    "directory": "js"
  },
  "homepage": "https://microgen.id",
  "scripts": {
    "build": "tsc && tsc-alias -p tsconfig.json",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch --verbose false --silent false",
    "postbuild": "esm2cjs --in dist/module --out dist/main -l error"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "sdk",
    "microgen",
    "microgen-client"
  ],
  "author": "Mejik",
  "license": "MIT",
  "devDependencies": {
    "@alcalzone/esm2cjs": "1.1.2",
    "@types/jest": "29.5.12",
    "@types/qs": "6.9.15",
    "@types/ws": "8.5.12",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "dotenv": "10.0.0",
    "jest": "29.7.0",
    "prettier": "3.3.3",
    "tsc-alias": "1.8.10",
    "ts-jest": "29.2.4",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "isomorphic-ws": "5.0.0",
    "qs": "6.13.0",
    "ws": "8.18.0"
  }
}
